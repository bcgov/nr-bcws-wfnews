{
  "index": "index.html",
  "assetGroups": [
    {
      "name": "wfnews",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.png", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "attachments",
      "urls": ["/**/wfdm-document-management-api/documents/*"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "10d"
      }
    }
  ]
}
