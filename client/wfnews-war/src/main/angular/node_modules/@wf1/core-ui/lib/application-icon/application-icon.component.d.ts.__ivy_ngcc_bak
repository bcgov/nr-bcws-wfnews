import { OnInit } from '@angular/core';
import { MatIconRegistry } from '@angular/material/icon';
import { DomSanitizer } from '@angular/platform-browser';
import { IconSize, IconType, IncidentType, IncidentTypeAgencyAssist, IncidentTypeFieldActivity, ReportOfFireType } from "./application-icon.model";
export declare class ApplicationIconComponent implements OnInit {
    iconType: IconType;
    iconSize: IconSize;
    hasHalo: boolean;
    colourCode: IncidentType | IncidentTypeAgencyAssist | IncidentTypeFieldActivity | ReportOfFireType;
    isBlinking: boolean;
    isCancelled: boolean;
    isAssignedToIncident: boolean;
    private HALO_SUFFIX;
    constructor(iconRegistry: MatIconRegistry, sanitizer: DomSanitizer);
    ngOnInit(): void;
    getHaloType(): string;
    getRootIconClasses(): string;
    getMainIconClasses(): string;
    getHaloIconClasses(): string;
}
