import { AfterViewInit, EventEmitter, OnInit, TemplateRef } from "@angular/core";
export declare enum MapControl {
    NONE = "none",
    LAYER_TREE = "layer-tree",
    BASEMAPS = "basemaps"
}
export declare class ApplicationToolbarComponent implements OnInit, AfterViewInit {
    menuTemplate: TemplateRef<any>;
    toolbarTemplate: TemplateRef<any>;
    selectedMapControl: EventEmitter<MapControl>;
    isLayersToggled: boolean;
    isBasemapsToggled: boolean;
    constructor();
    ngOnInit(): void;
    ngAfterViewInit(): void;
}
