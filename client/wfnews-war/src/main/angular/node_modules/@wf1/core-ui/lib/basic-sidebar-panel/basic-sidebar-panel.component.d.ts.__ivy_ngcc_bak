import { EventEmitter, OnInit } from "@angular/core";
import { TokenService } from "../services/token.service";
import { NavigationService } from "../services/navigation.service";
export declare class BasicSidebarPanelComponent implements OnInit {
    token: TokenService;
    navigationService: NavigationService;
    TOOLTIP_DELAY: number;
    titleText: string;
    linkIcon?: string;
    linkAlignLeft?: boolean;
    linkTitle?: string;
    showBackButton: boolean;
    linkClick: EventEmitter<string>;
    isInitializing: boolean;
    constructor(token: TokenService, navigationService: NavigationService);
    ngOnInit(): void;
    getTitleIconClasses(): string;
    getMainContentClasses(): string;
    titleLinkClicked(): void;
    titleBackClicked(): void;
}
