import { FilterConfig } from "../search/models/filter/filter-config";
import { BasicSidebarPanelComponent } from "../basic-sidebar-panel/basic-sidebar-panel.component";
import { DefaultSort } from "../search/models/sort/default-sort";
import { SortOption } from "../search/models/sort/sort-option";
export declare class FilterSidebarPanelComponent extends BasicSidebarPanelComponent {
    recordCount: number;
    filters?: FilterConfig[];
    defaultSort?: DefaultSort;
    sortOptions?: SortOption[];
    componentId: string;
    isInitializing: boolean;
    isShowingFilterOptions: boolean;
    getMainContentClasses(): string;
    setFilterSize(isShowingFilterOptions: boolean): void;
}
