import { EventEmitter, OnInit } from "@angular/core";
import { TokenService } from "../services/token.service";
import { FilterConfig } from "../search/models/filter/filter-config";
export declare class FullWidthPanelComponent implements OnInit {
    private token;
    titleText: string;
    linkIcon?: string;
    recordCount: number;
    filters?: FilterConfig[];
    linkClick: EventEmitter<string>;
    isInitializing: boolean;
    isShowingFilterOptions: boolean;
    constructor(token: TokenService);
    ngOnInit(): void;
    getMainContentClasses(): string;
    setFilterSize(isShowingFilterOptions: boolean): void;
    titleLinkClicked(): void;
}
