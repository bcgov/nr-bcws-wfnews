import { OnInit } from "@angular/core";
import { Store } from "@ngrx/store";
import { CodeTablesIndex } from './models/code-tables-index';
import { WildfireIncidentResource } from '@wf1/incidents-rest-api';
import { MapService } from "../services/map.service";
export declare class InfoIncidentPanelComponent implements OnInit {
    private store;
    private mapService;
    incident: WildfireIncidentResource;
    codeLabelLookupTables: CodeTablesIndex;
    constructor(store: Store<any>, mapService: MapService);
    ngOnInit(): void;
    getCodeLookupTables(): void;
    getLabel(table: string, value: string): string;
    getPanelClasses(): string;
    getFormattedLocation(): string;
}
