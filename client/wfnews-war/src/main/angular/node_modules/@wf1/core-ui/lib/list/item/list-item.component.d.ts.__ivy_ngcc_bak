import { EventEmitter, OnInit } from "@angular/core";
import { Info, ListItemConfig } from "./list-item-config";
import { IconSize, IconType, IncidentType, ReportOfFireType } from "../../application-icon/application-icon.model";
import { MapService } from "../../services/map.service";
export declare class InternalListItemConfig {
    icon: {
        type: IconType;
        size: IconSize;
        colourCode: IncidentType | ReportOfFireType;
        isBlinking: boolean;
        isCancelled: boolean;
        isAssignedToIncident: boolean;
    };
    time?: string;
    title: string;
    titleLevelInfo: Info;
    infoRows: [[Info, Info]];
    location: [number, number];
}
export declare class ListItemComponent implements OnInit {
    private mapService;
    TOOLTIP_DELAY: number;
    config: ListItemConfig;
    mapElement: string;
    itemSelect: EventEmitter<ListItemConfig>;
    internalConfig: InternalListItemConfig;
    constructor(mapService: MapService);
    ngOnInit(): void;
    handleMouseEvent(location?: [number, number]): void;
    handleClick(): void;
    zoomToPlace(location: [number, number]): void;
}
