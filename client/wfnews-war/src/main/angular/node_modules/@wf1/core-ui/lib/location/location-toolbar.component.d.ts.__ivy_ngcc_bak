import { EventEmitter, OnDestroy, OnInit } from "@angular/core";
import { AppConfigService } from "../services/app-config.service";
import { MapService } from "../services/map.service";
import { ValidatorFn } from "@angular/forms";
export declare class LocationToolbarComponent implements OnInit, OnDestroy {
    private config;
    private mapService;
    set location(location: [number, number]);
    locationSelect: EventEmitter<[number, number]>;
    TOOLTIP_DELAY: number;
    MAP_INSTANCE: string;
    constructor(config: AppConfigService, mapService: MapService);
    geometryValidator(mapService: any): ValidatorFn;
    ngOnInit(): void;
    ngOnDestroy(): void;
    zoomToSelectedLocation(): void;
    setAnchorFromMap(): void;
    clearSelectedLocation(): void;
}
