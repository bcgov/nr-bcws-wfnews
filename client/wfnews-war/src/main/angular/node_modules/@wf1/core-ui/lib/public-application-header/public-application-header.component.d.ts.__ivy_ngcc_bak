import { OnInit } from "@angular/core";
export declare class PublicAppHeaderActionItem {
    label: string;
    icon?: any;
    svgIcon?: any;
    badge?: number;
    callBackFunction?: Function;
    menuItems?: PublicAppHeaderMenuItem[];
}
export declare class PublicAppHeaderMenuItem {
    label: string;
    icon?: any;
    svgIcon?: any;
    badge?: number;
    callBackFunction?: Function;
}
export declare class PublicApplicationHeaderComponent implements OnInit {
    titleText: string;
    headerItems: PublicAppHeaderActionItem[];
    headerLogoPath: string;
    appVersion: string;
    userName: string;
    environment: string;
    acronym: string;
    buildNumber: string;
    constructor();
    ngOnInit(): void;
}
