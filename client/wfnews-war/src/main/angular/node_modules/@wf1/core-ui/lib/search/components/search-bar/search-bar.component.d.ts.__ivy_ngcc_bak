import { OnInit } from "@angular/core";
import { FormControl } from "@angular/forms";
import { SortComponent } from "../sort/sort.component";
import { Store } from "@ngrx/store";
import { SearchState } from "../../store/state";
import { IncidentType } from "../../../application-icon/application-icon.model";
export declare class SearchBarComponent implements OnInit {
    private store;
    TOOLTIP_DELAY: number;
    count: number;
    componentId: string;
    showSortButton: boolean;
    set sortModal(modal: SortComponent);
    searchControl: FormControl;
    clearFilterIconType: IncidentType;
    isResetSearch: boolean;
    constructor(store: Store<SearchState>);
    ngOnInit(): void;
    resetSearch(): void;
    listenSearchChanges(): void;
    refreshSearch(): void;
    updateSearch(searchTerm: string): void;
    toggleSortModal(): void;
    clearFilters(): void;
}
