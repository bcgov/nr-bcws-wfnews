import { HttpBackend } from "@angular/common/http";
import { Observable } from "rxjs";
import { LibraryConfig } from "../config/library-config";
import { ApplicationConfig } from "../interfaces/application-config";
export declare class AppConfigService {
    private httpHandler;
    private libConfig;
    private appConfig;
    private config;
    configEmitter: Observable<ApplicationConfig>;
    constructor(httpHandler: HttpBackend, libConfig: LibraryConfig);
    loadAppConfig(): Promise<void>;
    getConfig(): ApplicationConfig;
}
