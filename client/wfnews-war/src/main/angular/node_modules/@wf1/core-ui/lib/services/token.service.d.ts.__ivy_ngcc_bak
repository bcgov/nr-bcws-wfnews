import { Injector } from "@angular/core";
import { Observable } from "rxjs";
import { AppConfigService } from "./app-config.service";
export declare class TokenService {
    private injector;
    protected appConfigService: AppConfigService;
    private LOCAL_STORAGE_KEY;
    private useLocalStore;
    private oauth;
    private tokenDetails;
    private credentials;
    private authToken;
    credentialsEmitter: Observable<any>;
    authTokenEmitter: Observable<string>;
    constructor(injector: Injector, appConfigService: AppConfigService);
    checkForToken(redirectUri?: string, lazyAuth?: boolean, allowLocalExpiredToken?: boolean): void;
    isTokenExpired(token: any): boolean;
    private parseToken;
    private initImplicitFlow;
    initRefreshTokenImplicitFlow(authorizeURL: string, storageKey: string, errorCallback: any): Observable<any>;
    private initAuthFromSession;
    initAuth(response: any): void;
    private initAndEmit;
    updateToken(oauthToken: any): void;
    getOauthToken(): any;
    getTokenDetails(): any;
    doesUserHaveApplicationPermissions(scopes?: string[]): boolean;
    clearLocalStorageToken(): void;
    private handleError;
}
