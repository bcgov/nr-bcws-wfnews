import { ChangeDetectorRef, OnInit } from "@angular/core";
import { MapService } from "../services/map.service";
import { TreeNode } from "../interfaces/layer-treemap";
import { NestedTreeControl } from "@angular/cdk/tree";
import { MatTreeNestedDataSource } from "@angular/material/tree";
import { SelectionModel } from "@angular/cdk/collections";
export declare class WFMLLayerTreeComponent implements OnInit {
    private mapService;
    private changeDetectorRef;
    treeControl: NestedTreeControl<TreeNode>;
    dataSource: MatTreeNestedDataSource<TreeNode>;
    /** The selection for checklist */
    checklistSelection: SelectionModel<TreeNode>;
    constructor(mapService: MapService, changeDetectorRef: ChangeDetectorRef);
    ngOnInit(): void;
    initializeTreeLayer(): void;
    findSelectedElements(node: TreeNode): TreeNode[];
    hasChildren: (_: number, node: TreeNode) => boolean;
    /** Whether all the descendants of the node are selected */
    descendantsAllSelected(node: TreeNode): boolean;
    /** Whether part of the descendants are selected */
    descendantsPartiallySelected(node: TreeNode): boolean;
    /** Toggle the game selection. Select/deselect all the descendants node */
    nodeSelectionToggle(node: TreeNode): void;
}
