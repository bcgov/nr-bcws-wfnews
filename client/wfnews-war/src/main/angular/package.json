{
  "name": "wildfire",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve.local.proxy": "ng serve --disableHostCheck true",
    "start": "ng serve --proxy-config ../dm-proxy/proxy.conf.json",
    "start.prod": "ng serve --configuration production",
    "build": "ng build",
    "build.prod": "ng build --configuration production --base-href /wfnews/ && replace-in-file /ngsw:/g ngsw:wfimi: dist/ngsw-worker.js --verbose --isRegex",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cr": "npm run lint && npm start.prod",
    "mavenbuild": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production --output-path ../../../target/angular --base-href /pub/wfim/ && replace-in-file /ngsw:/g ngsw:wfimi: dist/ngsw-worker.js --verbose --isRegex",
    "mavenbuild-source-map": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production --output-path ../../../target/angular --base-href /pub/wfim/ --source-map && replace-in-file /ngsw:/g ngsw:wfimi: ../../../target/angular/ngsw-worker.js --verbose --isRegex"
  },
  "private": true,
