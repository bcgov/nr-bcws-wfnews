<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vividsolutions.wildfire" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HostedWebApp">
        <param name="ios-package" value="CDVHostedWebApp" />
        <param name="onload" value="true" />
    </feature>
    <name>WFONE</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://wildfire.vividsolutions.com/" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="fb:*" />
    <allow-navigation hap-rule="yes" href="https://wildfire.vividsolutions.com/*" />
    <allow-navigation hap-rule="yes" href="*://*twitter.com/*" />
    <allow-navigation hap-rule="yes" href="*://*gov.bc.ca/*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="29" src="/assets/icons/icon-29x29.png" width="29" />
    <icon height="40" src="/assets/icons/icon-40x40.png" width="40" />
    <icon height="58" src="/assets/icons/icon-58x58.png" width="58" />
    <icon height="60" src="/assets/icons/icon-60x60.png" width="60" />
    <icon height="72" src="/assets/icons/icon-72x72.png" width="72" />
    <icon height="76" src="/assets/icons/icon-76x76.png" width="76" />
    <icon height="80" src="/assets/icons/icon-80x80.png" width="80" />
    <icon height="100" src="/assets/icons/icon-100x100.png" width="100" />
    <icon height="120" src="/assets/icons/icon-120x120.png" width="120" />
    <icon height="144" src="/assets/icons/icon-144x144.png" width="144" />
    <icon height="152" src="/assets/icons/icon-152x152.png" width="152" />
    <icon height="180" src="/assets/icons/icon-180x180.png" width="180" />
    <splash height="960" src="/assets/splash-screens/splash-640x960.png" width="640" />
    <splash height="1136" src="/assets/splash-screens/splash-640x1136.png" width="640" />
    <splash height="1334" src="/assets/splash-screens/splash-750x1334.png" width="750" />
    <splash height="1024" src="/assets/splash-screens/splash-768x1024.png" width="768" />
    <splash height="768" src="/assets/splash-screens/splash-1024x768.png" width="1024" />
    <splash height="2208" src="/assets/splash-screens/splash-1242x2208.png" width="1242" />
    <splash height="2048" src="/assets/splash-screens/splash-1536x2048.png" width="1536" />
    <splash height="1536" src="/assets/splash-screens/splash-2048x1536.png" width="2048" />
    <splash height="1242" src="/assets/splash-screens/splash-2208x1242.png" width="2208" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="Fullscreen" value="false" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
        <param name="onload" value="true" />
    </feature>
    <feature name="UIInAppBrowser">
        <param name="ios-package" value="CDVUIInAppBrowser" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WKInAppBrowser">
        <param name="ios-package" value="CDVWKInAppBrowser" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SafariViewController">
        <param name="ios-package" value="SafariViewController" />
    </feature>
    <feature name="AppAvailability">
        <param name="ios-package" value="AppAvailability" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
</widget>
