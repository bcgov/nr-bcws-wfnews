{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wfone-public-mobile": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "wfone",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/wfone-public-mobile",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "fileReplacements": [
              {
                "replace": "src/config/appConfig.json",
                "with": "src/config/dev/appConfig.json"
              },
              {
                "replace": "src/config/bans-prohibitions-rss.xml",
                "with": "src/config/dev/bans-prohibitions-rss.xml"
              }
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/images",
              "src/config/appConfig.json",
              "src/config/bans-prohibitions-rss.xml",
              "src/manifest.json",
              "src/refresh-token.html",
              "src/privacy_policy.html",
              { "glob": "assetlinks.json", "input": "src/.well-known", "output": "/.well-known/" },
              {
                  "glob": "**/*",
                  "input": "node_modules/@qqnluaq/smk/dist/assets",
                  "output": "assets/smk/assets"
              },
              "src/announcements" 
            ],
            "styles": [
              "src/styles/main.scss"
            ],
            "scripts": [
              "node_modules/gsap/src/uncompressed/TweenMax.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/@qqnluaq/smk/dist/smk.js",
              "node_modules/marked/lib/marked.js",
              "src/assets/js/markdown-event-emitter.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/manifest.json",
              "src/refresh-token.html",
                "src/privacy_policy.html",
                { "glob": "assetlinks.json", "input": "src/.well-known", "output": "/.well-known/" },
                {
                  "glob": "**/*",
                  "input": "node_modules/@qqnluaq/smk/dist/assets",
                  "output": "assets/smk/assets"
                },
              "src/announcements" 

              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            },
            "production-mobile": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.mobile.ts"
                },
                {
                  "replace": "src/config/appConfig.json",
                  "with": "src/config/prod-mobile/appConfig.json"
                },
                {
                  "replace": "src/config/bans-prohibitions-rss.xml",
                  "with": "src/config/prod-mobile/bans-prohibitions-rss.xml"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/config/appConfig.json",
                "src/config/bans-prohibitions-rss.xml",
                "src/manifest.json",
                "src/refresh-token.html",
                "src/privacy_policy.html",
                { "glob": "assetlinks.json", "input": "src/.well-known", "output": "/.well-known/" },
                {
                  "glob": "**/*",
                  "input": "node_modules/@qqnluaq/smk/dist/assets",
                  "output": "assets/smk/assets"
                },
              "src/announcements" 


              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            },
            "int-mobile": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.int.mobile.ts"
                },
                {
                  "replace": "src/config/appConfig.json",
                  "with": "src/config/int-mobile/appConfig.json"
                },
                {
                  "replace": "src/config/bans-prohibitions-rss.xml",
                  "with": "src/config/int-mobile/bans-prohibitions-rss.xml"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/config/appConfig.json",
                "src/config/bans-prohibitions-rss.xml",
                "src/manifest.json",
                "src/refresh-token.html",
                "src/privacy_policy.html",
                { "glob": "assetlinks.json", "input": "src/.well-known", "output": "/.well-known/" },
                {
                  "glob": "**/*",
                  "input": "node_modules/@qqnluaq/smk/dist/assets",
                  "output": "assets/smk/assets"
                },
              "src/announcements" 


              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            },
            "tst-mobile": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.tst.mobile.ts"
                },
                {
                  "replace": "src/config/appConfig.json",
                  "with": "src/config/tst-mobile/appConfig.json"
                },
                {
                  "replace": "src/config/bans-prohibitions-rss.xml",
                  "with": "src/config/tst-mobile/bans-prohibitions-rss.xml"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/config/appConfig.json",
                "src/config/bans-prohibitions-rss.xml",
                "src/manifest.json",
                "src/refresh-token.html",
                "src/privacy_policy.html",
                { "glob": "assetlinks.json", "input": "src/.well-known", "output": "/.well-known/" },
                {
                  "glob": "**/*",
                  "input": "node_modules/@qqnluaq/smk/dist/assets",
                  "output": "assets/smk/assets"
                },
              "src/announcements" 


              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wfone-public-mobile:build",
            "port": 9200
          },
          "configurations": {
            "production": {
              "browserTarget": "wfone-public-mobile:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wfone-public-mobile:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles/main.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json",
              "src/refresh-token.html",
              "src/privacy_policy.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wfone-public-mobile-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "wfone-public-mobile:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wfone-public-mobile:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "wfone-public-mobile"
}
