  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.prod": "ng serve --prod",
    "build": "ng build",
    "build.prod": "ng build --prod --base-href /pub/wfone-public-mobile/ && replace-in-file /ngsw:/g ngsw:wfonepm: dist/wfone-public-mobile/ngsw-worker.js --verbose --isRegex",
    "test": "ng test",
    "test-and-coverage": "ng test --browsers ChromeHeadlessCustom --watch=false --code-coverage",
    "test-dev": "ng test --reporters=mocha",
    "test-dev-chrome": "ng test --watch=true --browsers=Chrome",
    "test-dev-safari": "ng test --watch=true --browsers=Safari",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cr": "npm run lint && npm start.prod",
    "mavenbuild": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --prod --output-path ../../../target/angular && replace-in-file /ngsw:/g ngsw:wfonepm: ../../../target/angular/ngsw-worker.js --verbose --isRegex",
    "bundle-report": "webpack-bundle-analyzer dist/wfone-public-mobile/stats.json",
    "compodoc": "npx compodoc -p src/tsconfig.app.json"
