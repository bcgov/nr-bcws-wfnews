<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
		<Appenders>
				<Console name="Console" target="SYSTEM_OUT"
						ignoreExceptions="false">
						<PatternLayout pattern="%-5p: [%c:%L] - %m%n" />
				</Console>
		</Appenders>
		<Loggers>
				<Root level="DEBUG">
						<AppenderRef ref="Console" />
				</Root>
		</Loggers>
</Configuration>
